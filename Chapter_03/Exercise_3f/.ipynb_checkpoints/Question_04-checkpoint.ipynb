    "import math\n",
    "n = 11\n",
    "together = math.factorial(n-1)\n",
    "combinations = math.factorial(n) / (2)\n",
    "separated = 1 - (together/combinations)\n",
    "print(\"P(separated) = \" + str(separated))"
