    "data = [30, 42, 65]\n",
    "weights = [5, 3, 2]\n",
    "weighted_sum = 0\n",
    "index = 0\n",
    "for weight in weights:\n",
    "    weighted_sum = weighted_sum + (weight * data[index])\n",
    "    index = index + 1\n",
    "weighted_mean = weighted_sum / (sum(weights))\n",
    "print(\"The weighted mean = \" + str(weighted_mean))"
